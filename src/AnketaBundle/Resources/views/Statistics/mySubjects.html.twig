{% extends 'AnketaBundle:Statistics:index.html.twig' %}

{% block navigation %}
    {% render "AnketaBundle:Statistics:menu" with { 'activeItems': [season.id, 'my_subjects'] } %}
{% endblock %}
{% block content %}
    <h2>Moje predmety</h2>
    <ul class="category-listing">    
    {% for subject in subjects %}
        <li><a href="{{ path('results_subject', { 'season_slug': season.slug, 'subject_code': subject.code }) }}">
            {{ subject.name }} ({{ subject.code }})
        </a>
        </li>
    {% endfor %}
    </ul>
{% endblock %}
