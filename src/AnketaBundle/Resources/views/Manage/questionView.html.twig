{% extends 'AnketaBundle:Manage:layoutQuestion.html.twig' %}

{% block question_content %}
    <h3>{{ question.question }}</h3>
    {% for option in question.options %}
        &nbsp &nbsp &nbsp &nbsp {{ option.option }} (eval: {{ option.evaluation }}) <br />
    {% endfor %}
    ID: {{ question.id }} <br />
    Kategoria: {{ question.category.category }}
{% endblock %}
