{% extends 'AnketaBundle:Manage:layoutQuestion.html.twig' %}

{% block content %}
    Vypisujem vsetky otazky. <br />
    {% for question in questions %}
            <b> {{ question.getId }}: </b>
            <a href="{{ path('view_question', { 'id': question.getId }) }}">{{ question.getQuestion }}</a><br />
    {% else %}
        Anketa nema ziadne otazky. Dakujeme za Vas cas.
    {% endfor %}
{% endblock %}