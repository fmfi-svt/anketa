{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% if collector.connection %}
        {% set icon %}
            <img width="24" height="28" alt="Libfajr" style="border-width: 0; vertical-align: middle; margin-right: 5px;" src="data:image/png;base64," />
        {% endset %}
        {% set text %}
            {{ collector.connection.requestCount }}
        {% endset %}
        {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
    {% endif %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img width="22" height="30" src="data:image/png;base64," alt="" /></span>
    <strong>Libfajr</strong>
    {% if collector.connection %}
        <span class="count">
            <span>{{ collector.connection.requestCount }}</span>
            <span>{{ '%0.0f'|format(collector.connection.totalTime * 1000) }} ms</span>
        </span>
    {% endif %}
</span>
{% endblock %}

{% block panel %}
    <h2>Connection statistics</h2>
    {% if collector.connection %}
        <table>
            <tr>
                <th>Requests</th>
                <td>{{ collector.connection.requestCount }}</td>
            </tr>
            <tr>
                <th>Errors</th>
                <td>{{ collector.connection.errorCount }}</td>
            </tr>
            <tr>
                <th>Total time</th>
                <td>{{ '%0.0f'|format(collector.connection.totalTime * 1000) }} ms</td>
            </tr>
            <tr>
                <th>Total downloaded bytes</th>
                <td>{{ collector.connection.downloadedBytes }}</td>
            </tr>
        </table>
    {% else %}
        <p>
            <em>No HTTP connection made or the connection does not support statistics</em>
        </p>
    {% endif %}
{% endblock %}
